n\r\n#aboutUs {\r\n  height: 100vh;\r\n  @include image.setImage(image.$concertRed, color.$black-dark);\r\n  @include position.flexRowCC();\r\n\r\n  #aboutInfo {\r\n    @include position.flexColumnCC();\r\n    gap: 20px;\r\n    margin: 0 20px;\r\n\r\n    h2 {\r\n      font-size: 1.7em;\r\n    }\r\n    p {\r\n      font-size: 1.1em;\r\n    }\r\n  }\r\n  img {\r\n    display: none;\r\n  }\r\n}\r\n\r\n@import \"media\";\r\n","scss\\_colors.scss":"$black-dark: #0a0808;\r\n$black-light: #161719;\r\n$red-dark: #8e1b1d;\r\n$red-light: #aa2a40;\r\n$blue-light: #2779d1;\r\n$blue-dark: #203a56;\r\n$white: rgb(248, 236, 236);\r\n","scss\\_positions.scss":"@mixin displayFlex {\r\n  display: flex;\r\n}\r\n\r\n@mixin flexColumnCC {\r\n  @include displayFlex();\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n@mixin flexRowCC {\r\n  @include displayFlex();\r\n  flex-direction: row;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n","scss\\_media.scss":"@media screen and (min-width: 800px) {\r\n  #aboutInfo + img {\r\n    display: block;\r\n    width: 100%;\r\n    object-fit: cover;\r\n    height: 500px;\r\n  }\r\n  #aboutInfo {\r\n    width: 100%;\r\n    max-width: 700px;\r\n    min-width: 400px;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"d82d33286a86750201eea21a17eb6966","cacheData":{"env":{}}}